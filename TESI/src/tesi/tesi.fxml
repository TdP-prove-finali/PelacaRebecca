                                    <Font size="14.0" />
                                 </font>
                                 <VBox.margin>
                                    <Insets left="5.0" top="20.0" />
                                 </VBox.margin>
                              </Label>
                              <Slider fx:id="sliderProb" blockIncrement="0.05" majorTickUnit="0.05" max="1.0" minorTickCount="1" showTickLabels="true" showTickMarks="true" snapToTicks="true" value="0.5">
